<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Components SYSTEM "XfireSkin.dtd">

<Components>
    <Component Name="XIGWebOverlay" Class="MainWindow" MinWidth="400" MinHeight="200" DefaultX="0" DefaultY="0" DefaultWidth="700" DefaultHeight="500">
        <Triggers>
            <Trigger Type="OnActiveMode">ShowTile</Trigger>
        </Triggers>
    </Component>
    

    <Component Name="XIGWebTitle" Class="Text" Stretch="ToContent" Events="Transparent">
        <Text Align="Right" Bold="True" Color="XIGOverlayTitleCG" Font="Arial" Size="11">%web_page_title:1399%</Text>
    </Component>
    
    <!-- This contains the 4 web buttons (exclude ADD TAB) and the URL -->
    <Component Name="XIGWebNaviContainer" Class="Tile" MinHeight="22" MaxHeight="22" Stretch="ToContent" Events="Transparent">
        <Triggers>
            <Trigger Type="OnActiveMode">ShowTile</Trigger>
            <Trigger Type="OnInActiveMode">HideTile</Trigger>
        </Triggers>
    </Component>
    
    <!-- URL -->
    <Component Name="XIGWebURLBG" Class="Tile" MinHeight="15" MaxHeight="15" Stretch="ToContent" Events="Transparent">
        <Images State="Normal">
            <Image Type="TopLeft">xig_web_url_background_left</Image>
            <Image Type="Top">xig_web_url_background_middle</Image>
            <Image Type="TopRight">xig_web_url_background_right</Image>
        </Images>
    </Component>
    
    <Component Name="XIGWebURL" Class="BrowserUrlEdit" Stretch="ToContent" CaretColor="XIGCaretCG" SelectionSelectorColor="XIGCaretCG" SelectionColor="ListColorSelectedCG" SelectionTextColor="ListColorSelectedTextCG" SelectionColor_InActive="TextFieldTextHighlightInactiveCG">
        <Fonts>
            <Font Type="Text" Bold="False" Color="TextFieldTextCG" Font="Arial" Size="12" />
        </Fonts>
    </Component>
    
    <Component Name="XIGWebContent" Class="BrowserControl" />
    
    <Component Name="XIGWebPageStatus" Class="Text" Stretch="ToContent" Events="Transparent">
        <Text Font="Arial" Size="11" Bold="False" Color="XIGOverlayTextCG">%web_page_status:1399%</Text>
    </Component>
    
    
    
    <!--
    //////////////////////////////////////////////
    // Tabs
    //////////////////////////////////////////////
    -->
    <Component Name="XIGWebTabBar" Class="Tile" MinHeight="26" MaxHeight="26" Stretch="ToContent" Events="Transparent">
        <Triggers>
            <Trigger Type="OnActiveMode">ShowTile</Trigger>
            <Trigger Type="OnInActiveMode">HideTile</Trigger>
        </Triggers>
        <Images State="Normal">
            <Image Type="Left">xig_web_tab_background_left</Image>
            <Image Type="Middle">xig_web_tab_background_middle</Image>
            <Image Type="Right">xig_web_tab_background_right</Image>
        </Images>
    </Component>
    
    <Component Name="XIGWebTabContentControl" Class="BrowserTabControl" Stretch="ToContent" ToolTipTile="XIGToolTipPopup">
        <Fonts>
            <Font Type="TabFont" Bold="False" Color="XIGWebTabInActiveTextCG" Font="Arial" Size="12" />
            <Font Type="ActiveTabFont" Bold="True" Color="XIGWebTabActiveTextCG" Font="Arial" Size="12" />
        </Fonts>
    </Component>
    
    <Component Name="XIGWebTab" Class="PushButton" MinWidth="90" MaxWidth="225" MinHeight="22" MaxHeight="22" Stretch="ToContent">
        <Images State="Normal">
            <Image Type="Left">xig_web_tab_left_norm</Image>
            <Image Type="Middle">xig_web_tab_middle_norm</Image>
            <Image Type="Right">xig_web_tab_right_norm</Image>
        </Images>
        <Images State="Hover">
            <Image Type="Left">xig_web_tab_left_hover</Image>
            <Image Type="Middle">xig_web_tab_middle_hover</Image>
            <Image Type="Right">xig_web_tab_right_hover</Image>
        </Images>
        <Images State="Down">
            <Image Type="Left">xig_web_tab_left_down</Image>
            <Image Type="Middle">xig_web_tab_middle_down</Image>
            <Image Type="Right">xig_web_tab_right_down</Image>
        </Images>
        <Images State="Focused">
            <Image Type="Left">xig_web_tab_left_focused</Image>
            <Image Type="Middle">xig_web_tab_middle_focused</Image>
            <Image Type="Right">xig_web_tab_right_focused</Image>
        </Images>
    </Component>
    
    
    
    <!--
    //////////////////////////////////////////////
    // Buttons
    //////////////////////////////////////////////
    -->
    <Component Name="XIGWebButtonBack" Class="BrowserBackButton" MinWidth="21" MaxWidth="21" MinHeight="18" MaxHeight="18" Stretch="ToContent" ToolTip="XIGWebButtonBackTip" DrawOnTopmost="True" TooltipTile="XIGToolTipPopup">
        <Images State="Normal">
            <Image Type="Middle">button_xig_web_back_norm</Image>
        </Images>
        <Images State="Hover">
            <Image Type="Middle">button_xig_web_back_hover</Image>
        </Images>
        <Images State="Down">
            <Image Type="Middle">button_xig_web_back_down</Image>
        </Images>
        <Images State="Disabled">
            <Image Type="Middle">button_xig_web_back_disabled</Image>
        </Images>
    </Component>
    
    <Component Name="XIGWebButtonForward" Class="BrowserForwardButton" MinWidth="21" MaxWidth="21" MinHeight="18" MaxHeight="18" Stretch="ToContent" ToolTip="XIGWebButtonForwardTip" DrawOnTopMost="True" TooltipTile="XIGToolTipPopup">
        <Images State="Normal">
            <Image Type="Middle">button_xig_web_forward_norm</Image>
        </Images>
        <Images State="Hover">
            <Image Type="Middle">button_xig_web_forward_hover</Image>
        </Images>
        <Images State="Down">
            <Image Type="Middle">button_xig_web_forward_down</Image>
        </Images>
        <Images State="Disabled">
            <Image Type="Middle">button_xig_web_forward_disabled</Image>
        </Images>
    </Component>
    
    <Component Name="XIGWebButtonRefresh" Class="BrowserRefreshButton" MinWidth="21" MaxWidth="21" MinHeight="18" MaxHeight="18" Stretch="ToContent" ToolTip="XIGWebButtonRefreshTip" DrawOnTopMost="True" TooltipTile="XIGToolTipPopup">
        <Images State="Normal">
            <Image Type="Middle">button_xig_refresh_norm</Image>
        </Images>
        <Images State="Hover">
            <Image Type="Middle">button_xig_refresh_hover</Image>
        </Images>
        <Images State="Down">
            <Image Type="Middle">button_xig_refresh_down</Image>
        </Images>
        <Images State="Disabled">
            <Image Type="Middle">button_xig_refresh_disabled</Image>
        </Images>
    </Component>
    
    <Component Name="XIGWebButtonHome" Class="BrowserHomeButton" MinWidth="21" MaxWidth="21" MinHeight="18" MaxHeight="18" Stretch="ToContent" ToolTip="XIGWebButtonHomeTip" DrawOnTopMost="True" TooltipTile="XIGToolTipPopup">
        <Images State="Normal">
            <Image Type="Middle">button_xig_web_home_norm</Image>
        </Images>
        <Images State="Hover">
            <Image Type="Middle">button_xig_web_home_hover</Image>
        </Images>
        <Images State="Down">
            <Image Type="Middle">button_xig_web_home_down</Image>
        </Images>
        <Images State="Disabled">
            <Image Type="Middle">button_xig_web_home_disabled</Image>
        </Images>
    </Component>
    
    <Component Name="XIGWebButtonTabClose" Class="PushButton" MinWidth="8" MaxWidth="8" MinHeight="8" MaxHeight="8" Stretch="ToContent">
        <Images State="Normal">
            <Image Type="Middle">button_xig_web_close_tab_norm</Image>
        </Images>
        <Images State="Hover">
            <Image Type="Middle">button_xig_web_close_tab_hover</Image>
        </Images>
        <Images State="Down">
            <Image Type="Middle">button_xig_web_close_tab_down</Image>
        </Images>
    </Component>
    
    <Component Name="XIGWebButtonNewTab" Class="BrowserNewTabButton" MinWidth="22" MaxWidth="22" MinHeight="22" MaxHeight="22" Stretch="ToContent" ToolTip="XIGWebButtonNewTabTip" ToolTipTile="XIGToolTipPopup" DrawOnTopMost="True">
        <Images State="Normal">
            <Image Type="Middle">button_xig_web_add_tab_norm</Image>
        </Images>
        <Images State="Hover">
            <Image Type="Middle">button_xig_web_add_tab_hover</Image>
        </Images>
        <Images State="Down">
            <Image Type="Middle">button_xig_web_add_tab_down</Image>
        </Images>
        <Images State="Disabled">
            <Image Type="Middle">button_xig_web_add_tab_down</Image>
        </Images>
    </Component>
    
    <Component Name="XIGWebPreviousTab" MinWidth="19" MaxWidth="19" MinHeight="22" MaxHeight="22" Class="PushButton" Stretch="ToContent">
        <Images State="Normal">
            <Image Type="Middle">button_xig_web_previous_tab_norm</Image>
        </Images>
        <Images State="Hover">
            <Image Type="Middle">button_xig_web_previous_tab_hover</Image>
        </Images>
        <Images State="Down">
            <Image Type="Middle">button_xig_web_previous_tab_down</Image>
        </Images>
    </Component>
    
    <Component Name="XIGWebNextTab" MinWidth="19" MaxWidth="19" MinHeight="22" MaxHeight="22" Class="PushButton" Stretch="ToContent">
        <Images State="Normal">
            <Image Type="Middle">button_xig_web_next_tab_norm</Image>
        </Images>
        <Images State="Hover">
            <Image Type="Middle">button_xig_web_next_tab_hover</Image>
        </Images>
        <Images State="Down">
            <Image Type="Middle">button_xig_web_next_tab_down</Image>
        </Images>
    </Component>
</Components>