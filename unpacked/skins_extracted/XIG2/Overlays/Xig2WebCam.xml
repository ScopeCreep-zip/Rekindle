<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE Overlay SYSTEM "XfireSkin.dtd"><Overlay>    <Tile Name="WebCam" ResizeX="100" ResizeY="100" Features="WebCam" DisplayName="WebCam" Component="XIGWebCamOverlay">        <!--        //////////////////////////////////////////////        // Window Frame        //////////////////////////////////////////////        -->        <Tile Name="TopRow" JustY="Top" ResizeX="100" Component="XIGLiveStatsTopRow" />        <Tile Name="LeftSide" JustX="Left" JustY="Top" ResizeY="100" IndentTop="TopRow.Height" IndentBottom="BottomRow.Height" Component="XIGLiveStatsLeftSide" />        <Tile Name="Background" Z="0" ResizeX="100" ResizeY="100" IndentRight="RightSide.Width" IndentBottom="BottomRow.Height" IndentLeft="LeftSide.Width" IndentTop="TopRow.Height" Component="XIGLiveStatsBackground" />        <Tile Name="RightSide" JustX="Right" JustY="Top" ResizeY="100" IndentTop="TopRow.Height" IndentBottom="BottomRow.Height" Component="XIGLiveRightSide" />        <Tile Name="BottomRow" JustY="Bottom" ResizeX="100" Component="XIGLiveBottomRow" />                                <!--        //////////////////////////////////////////////        // Content        //////////////////////////////////////////////        -->        <Tile Name="WebCamControl" Z="25" ResizeX="100" ResizeY="100" IndentTop="25" IndentRight="25" IndentBottom="25" IndentLeft="25" Component="XIGWebCamControl" />        <Tile Name="AdjustWebCamText" Y="WebCamControl.Bottom+2" Z="26" JustX="Center" Component="XIGWebCamAdjustText" />    </Tile></Overlay>