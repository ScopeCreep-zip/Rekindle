<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Tile SYSTEM "XfireSkin.dtd">

<Tile Name="MainWindow" ResizeX="100" ResizeY="100" Component="MainWindow">
    <!--
    //////////////////////////////////////////////
    // Resize Grippers
    //////////////////////////////////////////////
    -->
    <Tile Name="ResizeGripperTL" Z="26" JustX="Left" JustY="Top" Component="MainResizeGripperTL" />
    <Tile Name="ResizeTop" Z="25" ResizeX="100" IndentLeft="ResizeGripperTL.Width" IndentRight="ResizeGripperTR.Width+13" Component="MainResizeGripperTop" />
    <Tile Name="ResizeGripperTR" X="13" Z="26" JustX="Right" JustY="Top" Component="MainResizeGripperTR" />
    <Tile Name="ResizeL" Z="25" ResizeY="100" IndentTop="ResizeGripperTL.Height" IndentBottom="ResizeGripperBL.Height" Component="MainResizeGripperL" />
    <Tile Name="ResizeR" X="13" Z="25" JustX="Right" ResizeY="100" IndentTop="ResizeGripperTR.Height" IndentBottom="ResizeGripperBR.Height" Component="MainResizeGripperR" />
    <Tile Name="ResizeGripperBL" Z="26" JustX="Left" JustY="Bottom" Component="MainResizeGripperBL" />
    <Tile Name="ResizeBottom" Z="25" JustX="Center" JustY="Bottom" ResizeX="100" IndentLeft="ResizeGripperBL.Width" IndentRight="ResizeGripperBR.Width+13" Component="MainResizeGripperBottom" />
    <Tile Name="ResizeGripperBR" X="13" Z="26" JustX="Right" JustY="Bottom" Component="MainResizeGripperBR" />
    
    
    
    <!--
    //////////////////////////////////////////////
    // Window Draggers
    //////////////////////////////////////////////
    -->
    <Tile Name="WindowDraggerTop" Z="10" ResizeX="100" ResizeY="100" IndentRight="13" Component="WindowDragger" />
    
    
    
    <!--
    //////////////////////////////////////////////
    // Window Frame
    //////////////////////////////////////////////
    -->
    <Tile Name="TopRow" Z="1" ResizeX="100" IndentRight="13" Component="MainTopRow" />
    <Tile Name="LeftSide" Z="1" JustX="Left" ResizeY="100" IndentTop="TopRow.Height" IndentBottom="BottomRow.Height" Component="MainLeftSide" />
    <Tile Name="Background" Z="1" ResizeX="100" ResizeY="100" IndentTop="TopRow.Height" IndentRight="RightSide.Width+13" IndentBottom="BottomRow.Height" IndentLeft="LeftSide.Width" Component="MainBackground" />
    <Tile Name="RightSide" X="13" Z="1" JustX="Right" ResizeY="100" IndentTop="TopRow.Height" IndentBottom="BottomRow.Height" Component="MainRightSide" />
    <Tile Name="BottomRow" Z="1" JustY="Bottom" ResizeX="100" IndentRight="13" Component="MainBottomRow" />
    
    
    
    <!--
    //////////////////////////////////////////////
    // Content
    //////////////////////////////////////////////
    -->
    <Tile Name="InfoBar" Y="5" Z="24" ResizeX="100" IndentLeft="5" IndentRight="18" Component="InfoBar">
        <!--[BEGIN] Xfire Logo -->
        <Tile Name="XfireLogo" X="4" Y="5" Z="99" Component="XfireLogo" />
        <!--[END] Xfire Logo -->
        
        
        <!--[BEGIN] Window Control -->
        <Tile Name="ButtonMinimize" X="49" Y="3" Z="25" JustX="Right" Component="ButtonMinimize" />
        <Tile Name="ButtonMaximize" X="25" Y="3" Z="25" JustX="Right" Component="ButtonMaximize" />
        <Tile Name="ButtonClose" X="5" Y="3" Z="25" JustX="Right" Component="ButtonClose" />
        <!--[END] Window Control -->
    </Tile>
    
    
    <!--[BEGIN] Game Logo -->
    <!-- <Tile Name="GameLogo" Z="30" JustX="Center" Component="GameLogo" /> -->
    <!--[END] Game Logo -->
    
    
    <!--[BEGIN] Menus -->
        <Tile Name="MenuBackground" Y="InfoBar.Bottom+1" Z="24" ResizeX="100" IndentLeft="5" IndentRight="18" Component="MenuBackground">
            <Tile Name="XfireMenu" X="3" Z="25" Component="XfireMenu" />
            <Tile Name="ToolsMenu" X="XfireMenu.Right+2" Z="25" Component="ToolsMenu" />
            <Tile Name="HelpMenu" X="ToolsMenu.Right+2" Z="35" Component="HelpMenu" />
        </Tile>
    <!--[END] Menus -->
    
    
    <!--[BEGIN] Username -->
    <Tile Name="UsernameContainer" X="7" Y="MenuBackground.Bottom+8" Z="25" ResizeX="100" IndentRight="18" Component="UsernameContainer">
        <Tile Name="Username" Z="26" JustX="Left" JustY="Center" ResizeX="100" Component="Username" />
    </Tile>
    <!--[END] Username -->
    
    
    <!--[BEGIN] Status -->
    <Tile Name="StatusBarContainer" Y="UsernameContainer.Bottom+9" Z="25" ResizeX="100" IndentRight="18" IndentLeft="5" Component="StatusBarContainer">
        <Tile Name="Status" X="5" Y="3" Z="26" ResizeX="100" IndentRight="18" Component="Status" />
        <Tile Name="StatusChanger" X="Status.Right" Y="1" Z="26" Component="ButtonStatusChanger" />
    </Tile>
    <!--[END] Status -->
    
    
    <!--[BEGIN] Ad -->
    <!-- <Tile Name="AdViewContainer" Z="20" ResizeX="100" Component="AdViewContainer"> -->
        <Tile Name="AdView" Y="StatusBarContainer.Bottom+1" Z="99" JustX="Center" Component="AdView" />
    <!-- </Tile> -->
    <!--[END] Ad -->
    
    
    <!--[BEGIN] Tabs -->
    <Tile Name="TabsContainer" Y="AdView.Bottom+1" Z="25" ResizeX="100" IndentRight="18" IndentLeft="5" Component="TabButtonsContainer">
        <Tile Name="ButtonFriendsTab" Y="2" Z="26" Component="ButtonFriendsTab" />
        <Tile Name="TabSeparator1" X="ButtonFriendsTab.Right" Z="26" Component="TabVerticalSeparator" />
        <Tile Name="ButtonChatRoomsTab" X="TabSeparator1.Right" Y="ButtonFriendsTab.Top" Z="26" Component="ButtonChatRoomsTab" />
        <Tile Name="TabSeparator2" X="ButtonChatRoomsTab.Right" Z="26" Component="TabVerticalSeparator" />
        <Tile Name="ButtonServersTab" X="TabSeparator2.Right" Y="ButtonFriendsTab.Top" Z="26" Component="ButtonServersTab" />
        <Tile Name="TabSeparator3" X="ButtonServersTab.Right" Z="26" Component="TabVerticalSeparator" />
        <Tile Name="ButtonFilesTab" X="TabSeparator3.Right" Y="ButtonFriendsTab.Top" Z="26" Component="ButtonFilesTab" />
        <Tile Name="TabSeparator4" X="ButtonFilesTab.Right" Z="26" Component="TabVerticalSeparator" />
        <Tile Name="ButtonPicturesTab" X="TabSeparator4.Right" Y="ButtonFriendsTab.Top" Z="26" Component="ButtonPicturesTab" />
        <Tile Name="TabSeparator5" X="ButtonPicturesTab.Right" Z="26" Component="TabVerticalSeparator" />
        <Tile Name="ButtonVideosTab" X="TabSeparator5.Right" Y="ButtonFriendsTab.Top" Z="26" Component="ButtonVideosTab" />
        <Tile Name="TabSeparator6" X="ButtonVideosTab.Right" Z="26" Component="TabVerticalSeparator" />
    </Tile>
    <Tile Name="ButtonNotificationsTab" X="20" Y="MenuBackground.Bottom+3" Z="26" JustX="Right" Component="ButtonNotificationsTab">
        <Tile Name="ButtonNotificationsTabControl" Z="27" JustX="Center" JustY="Center" Component="ButtonNotificationsTabControl">
            <Tile Name="ButtonNotificationsTabVisited" Z="28" JustX="Center" JustY="Center" Component="ButtonNotificationsTabVisited" />
            <Tile Name="ButtonNotificationsTabUnvisited" Z="28" JustX="Center" JustY="Center" Component="ButtonNotificationsTabUnvisited" />
        </Tile>
    </Tile>
    <!--[END] Tabs -->
    
    
    <!--[BEGIN] Content Container -->
    <!-- <Tile Name="ContentContainer"  Component="ContentContainer"> -->
        <!--[BEGIN] Xfire View -->
        <Tile Name="XfireView" Y="TabsContainer.Bottom+1" Z="99" ResizeX="100" ResizeY="100" IndentRight="18" IndentBottom="91" IndentLeft="5" Component="XfireView" />
        <!--[END] Xfire View -->
        
        <!--[BEGIN] Xfire List Background -->
        <!-- <Tile Name="XfireListBackdrop" Z="98" ResizeX="100" ResizeY="100" IndentTop="1" IndentRight="1" IndentBottom="1" IndentLeft="1" Component="XfireListBackdrop" /> -->
        <!--[END] Xfire List Background -->
    <!-- </Tile> -->
    <!--[END] Content Container -->
    
    
    <!--[BEGIN] Tabbed Buttons -->
    <Tile Name="TabsButtonsContainer" Y="XfireView.Bottom+2" Z="25" ResizeX="100" IndentRight="18" IndentLeft="5" Component="TabControl">
        <Tile Name="FriendsButtonBar" ResizeX="100" ResizeY="100" Component="FriendsButtonBar">
            <Tile Name="FriendChatButton" X="1" Y="1" Z="30" Component="ButtonChatFriend" />
            <Tile Name="Separator1" X="FriendChatButton.Right" Z="30" Component="ButtonBarVerticalSeparator" />
            <Tile Name="FriendInfoButton" X="Separator1.Right" Y="FriendChatButton.Top" Z="30" Component="ButtonInfo" />
            <Tile Name="Separator2" X="FriendInfoButton.Right" Z="30" Component="ButtonBarVerticalSeparator" />
            <Tile Name="FriendJoinGame" X="Separator2.Right" Y="FriendChatButton.Top" Z="30" Component="ButtonJoinGame" />
            <Tile Name="Separator3" X="FriendJoinGame.Right" Z="30" Component="ButtonBarVerticalSeparator" />
            <Tile Name="FriendAddButton" Y="4" Z="30" JustX="Center" JustY="Bottom" Component="ButtonAddFriendLarge" />
        </Tile>
        <Tile Name="ChatRoomsButtonBar" ResizeX="100" ResizeY="100" Component="ChatRoomsButtonBar">
            <Tile Name="RoomEditBG" X="3" Y="13" Component="RoomEditBackground">
                <Tile Name="ChatRoomEdit" ResizeX="100" ResizeY="100" IndentTop="1" IndentRight="1" IndentBottom="1" IndentLeft="1" Component="RoomEdit" />
            </Tile>
            <Tile Name="ChatRoomEnter" X="RoomEditBG.Right" Y="1"  Z="30" Component="ButtonEnterChatRoom" />
            <Tile Name="Separator1" X="ChatRoomEnter.Right" Component="ButtonBarVerticalSeparator" />
            <Tile Name="ChatRoomDelete" X="Separator1.Right" Y="ChatRoomEnter.Top" Z="30" Component="ButtonDeleteChatRoom" />
            <Tile Name="Separator2" X="ChatRoomDelete.Right" Z="30" Component="ButtonBarVerticalSeparator" />
            <Tile Name="ChatRoomCreate" Y="4" Z="30" JustX="Center" JustY="Bottom" Component="ButtonCreateChatRoomLarge" />
        </Tile>
        <Tile Name="FilesButtonBar" ResizeX="100" ResizeY="100" Component="FilesButtonBar">
            <Tile Name="OpenFiles" X="1" Y="1" Z="30" Component="ButtonOpenFiles" />
            <Tile Name="Separator1" X="OpenFiles.Right" Z="30" Component="ButtonBarVerticalSeparator" />
            <Tile Name="DeleteFiles" X="Separator1.Right" Y="OpenFiles.Top" Z="30" Component="ButtonDeleteFiles" />
            <Tile Name="Separator2" X="DeleteFiles.Right" Z="30" Component="ButtonBarVerticalSeparator" />
            <Tile Name="SearchFiles" Y="4" Z="30" JustX="Center" JustY="Bottom" Component="ButtonSearchFilesLarge" />
        </Tile>
        <Tile Name="ServersButtonBar" ResizeX="100" ResizeY="100" Component="ServersButtonBar">
            <Tile Name="FavoriteServer" X="1" Y="1" Z="30" Component="ButtonFavoriteServers" />
            <Tile Name="Separator1" X="FavoriteServer.Right" Z="30" Component="ButtonBarVerticalSeparator" />
            <Tile Name="JoinGame" X="Separator1.Right" Y="FavoriteServer.Top" Z="30" Component="ButtonJoinGameServers" />
            <Tile Name="Separator2" X="JoinGame.Right" Z="30" Component="ButtonBarVerticalSeparator" />
            <Tile Name="RefreshServer" Y="4" Z="30" JustX="Center" JustY="Bottom" Component="ButtonRefreshServersLarge" />
        </Tile>
        <Tile Name="PicturesButtonBar" ResizeX="100" ResizeY="100" Component="PicturesButtonBar">
            <Tile Name="OpenPictures" X="1" Y="1" Z="30" Component="ButtonViewPictures" />
            <Tile Name="Separator1" X="OpenPictures.Right" Z="30" Component="ButtonBarVerticalSeparator" />
            <Tile Name="UploadPictures" X="Separator1.Right" Y="OpenPictures.Top" Z="30" Component="ButtonUploadPictures" />
            <Tile Name="Separator2" X="UploadPictures.Right" Z="30" Component="ButtonBarVerticalSeparator" />
            <Tile Name="DeletePictures" X="Separator2.Right" Y="OpenPictures.Top" Z="30" Component="ButtonDeletePictures" />
            <Tile Name="Separator3" X="DeletePictures.Right" Z="30" Component="ButtonBarVerticalSeparator" />
            <Tile Name="ViewPictures" Y="4" Z="30" JustX="Center" JustY="Bottom" Component="ButtonInfoPicturesLarge" />
        </Tile>
        <Tile Name="VideosButtonBar" ResizeX="100" ResizeY="100" Component="VideosButtonBar">
            <Tile Name="OpenVideos" X="1" Y="1" Z="30" Component="ButtonViewVideos" />
            <Tile Name="Separator1" X="OpenVideos.Right" Z="30" Component="ButtonBarVerticalSeparator" />
            <Tile Name="DeleteVideos" X="Separator1.Right" Y="OpenVideos.Top" Z="30" Component="ButtonDeleteVideos" />
            <Tile Name="Separator2" X="DeleteVideos.Right" Z="30" Component="ButtonBarVerticalSeparator" />
            <Tile Name="UploadVideos" X="Separator2.Right" Y="OpenVideos.Top" Z="30" Component="ButtonUploadVideos" />
            <Tile Name="Separator3" X="UploadVideos.Right" Z="30" Component="ButtonBarVerticalSeparator" />
            <Tile Name="SearchVideos" Y="4" Z="30" JustX="Center" JustY="Bottom" Component="ButtonSearchVideosLarge" />
        </Tile>
        <Tile Name="NotificationsButtonBar" ResizeX="100" ResizeY="100" Component="NotificationsButtonBar">
            <Tile Name="OpenNotification" X="1" Y="1" Component="ButtonOpenFiles" />
            <Tile Name="Separator1" X="OpenNotification.Right" Component="ButtonBarVerticalSeparator" />
            <Tile Name="DeleteNotification" X="Separator1.Right" Y="OpenNotification.Top" Component="ButtonDeleteFiles" />
            <Tile Name="Separator2" X="DeleteNotification.Right" Component="ButtonBarVerticalSeparator" />
        </Tile>
    </Tile>
    <!--[END] Tabbed Buttons -->
    
    
    <!--[BEGIN] Info View -->
    <Tile Name="MainInfoViewButton" Z="30" JustX="Right" JustY="Center" Component="ButtonMainInfoOut" />
    <!--[END] Info View -->
</Tile>