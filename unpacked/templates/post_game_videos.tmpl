		function OnUploadFailed(nIndex)
		{
			var strUploadedCheckId = getUploadedCheckId(nIndex);
			var strUploadCheckId = getUploadCheckId(nIndex);
			document.getElementById(strUploadedCheckId).checked = true;
			document.getElementById(strUploadCheckId).value = "%text_upload_failed%";
			document.getElementById(strUploadCheckId).disabled = false;
			document.getElementById(strUploadCheckId).style.color = "red";
		}

		function OnUploadVerifying(nIndex)
		{
			var strUploadedCheckId = getUploadedCheckId(nIndex);
			var strUploadCheckId = getUploadCheckId(nIndex);
			document.getElementById(strUploadedCheckId).checked = true;
			document.getElementById(strUploadCheckId).value = "%text_upload_verifying%";
			document.getElementById(strUploadCheckId).disabled = true;
		}
		
		function OnUpload(nIndex)
		{
			var strUploadedCheckId = getUploadedCheckId(nIndex);
			var strUploadCheckId = getUploadCheckId(nIndex);
			document.getElementById(strUploadedCheckId).checked = false;
			document.getElementById(strUploadCheckId).value = "%text_upload%";
			document.getElementById(strUploadCheckId).disabled = false;
		}

		function IsAdultChecked(nIndex)
		{
			var strAdultCheckId = getAdultContentCheckId(nIndex);
			return GetCheckedHelper(strAdultCheckId);
		}

		function SetNewFilePath(strPath)
		{
			bNewPreviewFile = true;
			strNewPath = strPath;
		}

		function SetNewPreviewFile(nIndex)
		{
			if (bNewPreviewFile)
			{
				var strPrevImgId = getPrevImgId(nIndex);
				document.getElementById(strPrevImgId).value = strNewPath;
				var strImgId = getImgId(nIndex);
				document.getElementById(strImgId).src = strNewPath;
				bNewPreviewFile = false;
			}
		}

		function GetTextHelper(strElementId, nMaxLength)
		{
			var text_element = document.getElementById(strElementId);
			if (text_element)
			{
				if (text_element.value.length > nMaxLength)
				{
					return text_element.value.substr(0, nMaxLength);
				}
				else
				{
					return text_element.value;
				}
			}
			return "";
		}

		function GetTitle(nIndex)
		{
			var strTitleId = getTitleId(nIndex);
			var title_element = document.getElementById(strTitleId);
			if (title_element)
			{
				if (title_element.value.length > 255)
				{
					return title_element.value.substr(0, 255);
				}
				else
				{
					return title_element.value;
				}
			}
			return "";
		}

		function GetDescription(nIndex)
		{
			var strDescId = getDescriptionId(nIndex);
			var desc_element = document.getElementById(strDescId);
			if (desc_element)
			{
				if (desc_element.value.length > 1024)
				{
					return desc_element.value.substr(0, 1024);
				}
				else
				{
					return desc_element.value;
				}
			}
			return "";
		}

		function GetTags(nIndex)
		{
			var strTagsId = getTagsId(nIndex);
			return GetTextHelper(strTagsId, 255);
		}

		function GetGameName(nIndex)
		{
			var strGameId = getGameId(nIndex);
			return GetTextHelper(strGameId, 255);
		}

		function GetGameIndex(nIndex)
		{
			var strGameId = getGameId(nIndex);
			var intSelected = document.getElementById(strGameId).selectedIndex;
			return document.getElementById(strGameId).selectedIndex.toString();
		}

		function DisableGameName(nIndex)
		{
			var strGameId = getGameId(nIndex);
			var game_name_element = document.getElementById(strGameId);
			game_name_element.disabled = true;
		}

		function GetContest(nIndex)
		{
			var strContestId = getContestId(nIndex);
			var contest_element = document.getElementById(strContestId);
			if (contest_element)
			{
				var nSelectedIndex = contest_element.selectedIndex;
				return contest_element.options[nSelectedIndex].value;
			}
			return 0;
		}

