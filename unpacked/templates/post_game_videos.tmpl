			document.write("<tr class='header'><td colspan='2' align='right'>");
			document.write("	<div class='selection_links'>Upload:&nbsp;");
			document.write("		<span class='fakelink' onClick='OnUploadAllClicked();'>All</span>,&nbsp;");
			document.write("		<span class='fakelink' onClick='OnUploadNoneClicked();'>None</span></div>");
			document.write("	<div class='selection_links'>Delete:&nbsp;");
			document.write("		<span class='fakelink' onClick='OnDeleteAllClicked();'>All</span>,&nbsp;");
			document.write("		<span class='fakelink' onClick='OnDeleteNoneClicked();'>None</span></div>");
			document.write("</td></tr>");
