			var strTagsId = getTagsId(nIndex);
			var strContestId = getContestId(nIndex);
			var strContestHrefId = getContestHrefId(nIndex);
			var strAdultContentCheckId = getAdultContentCheckId(nIndex);
			var strOverlayId = getOverlayId(nIndex);
			var strPrevImgId = getPrevImgId(nIndex);
			var strPrevBtnId = getPrevBtnId(nIndex);
			var strGameId = getGameId(nIndex);
			var strNoContests = "&lt;%text_no_contests%&gt;";
			var strContestDetails = "%text_contest_details%";
			var nIndex;

			var str  = '<div class="video_object">';
			str += '	<table>';
			str += '		<tbody>';
			str += '			<tr>';
			str += '				<td class="icon_td">';
			str += '					<div class="icon_ctn">';
			str += '						<div class="img_element">';
			str += '							<div class="dropshadow_container" style="position:relative;">';
			str += '								<a onclick="return CanOpenImage();" href="' + strVideoURL + '">';
			str += '								<div style="position:relative;">';
			str += '									<div class="video_length">' + strLength + ': ' + strSize + '</div>';
			str += '									<div class="video_playbtn"></div>';
			str += '									<img width="120" height="90" src="' + strImgURL + '" class="image_normal" id="'+strImgId+'"/>';
			str += '								</div>';
			str += '								</a>';
			str += '							</div>';
			str += '						</div>';
			str += '						<div class="center_text">';
			str += '							<input type="checkbox" style="display:none" name="' +strDeleteCheckId+ '" id="' +strDeleteCheckId+ '"/>';
			str += '							<input type="button" onclick="OnDeleteRestore(document.getElementById(\'' +strDeleteCheckId+ '\'),document.getElementById(\'' +strUploadCheckId+ '\'),document.getElementById(\'' +strOverlayId+ '\'),this);" ondblclick="this.click()" value="%text_delete_video%" class="button delete" style="position: relative; z-index: 5;border: 1px solid #ccc;"/>';
			str += '						</div>';
			str += '					</div>';
			str += '				</td>';
