		function OnUploadToYoutube(objCheckbox, obj)
		{
			objCheckbox.checked = !objCheckbox.checked;
			if(objCheckbox.checked)
				obj.className = obj.className + ' checked';
			else
				obj.className = obj.className.replace('checked', '');
		}

		function OnDeleteRestore(objCheckbox, objUploadButton, objOverlayId, objDeleteButton)
		{
			objCheckbox.checked = !objCheckbox.checked;
			if (objCheckbox.checked)
			{
				objDeleteButton.value = "%text_restore%";
				objDeleteButton.className = objDeleteButton.className + ' restore';
				objUploadButton.disabled = true;
				objOverlayId.style.display = "";
				objOverlayId.style.visibility = "visible";
			}
			else
			{
				objDeleteButton.value = "%text_delete_video%";
				objDeleteButton.className = objDeleteButton.className.replace('restore', '');          
				objUploadButton.disabled = false;
				objOverlayId.style.display = "none";
				objOverlayId.style.visibility = "hidden";
			}
		}

		function OnUploadDone(nIndex)
		{
			var strUploadCheckId = getUploadCheckId(nIndex);
			document.getElementById(strUploadCheckId).value = "%text_upload_complete%";
			document.getElementById(strUploadCheckId).disabled = true;
		}

		function OnUploadClicked(checkbox_element)
		{
			var strClassName = "disabled_color";
			var bDisabled = true;
			if (checkbox_element.checked)
			{
				strClassName = "";
				bDisabled = false;
			}

			var strTitleId = getTitleId(checkbox_element.index);
			var title_element = document.getElementById(strTitleId);
			if (title_element)
			{
				title_element.className = strClassName;
				title_element.disabled = bDisabled;
			}

			var strDescId = getDescriptionId(checkbox_element.index);
			var desc_element = document.getElementById(strDescId);
			if (desc_element)
			{
				desc_element.className = strClassName;
				desc_element.disabled = bDisabled;
			}

			var strTagsId = getTagsId(checkbox_element.index);
			var tags_element = document.getElementById(strTagsId);
			if (tags_element)
			{
				tags_element.className = strClassName;
				tags_element.disabled = bDisabled;
			}

			var strContestId = getContestId(checkbox_element.index);
			var contest_element = document.getElementById(strContestId);
			if (contest_element && bHasVideoContests)
			{
				contest_element.className = strClassName;
				contest_element.disabled = bDisabled;
			}

			var strAdultId = getAdultContentCheckId(checkbox_element.index);
			var adult_check_element = document.getElementById(strAdultId);
			if (adult_check_element)
			{
				adult_check_element.disabled = bDisabled;
			}
		}

		function OnDeleteClicked(checkbox_element)
		{
			var strUploadCheckId = getUploadCheckId(checkbox_element.index);
			var upload_check_element = document.getElementById(strUploadCheckId);

			var strTitleId = getTitleId(checkbox_element.index);
			var title_element = document.getElementById(strTitleId);

			var strDescId = getDescriptionId(checkbox_element.index);
			var desc_element = document.getElementById(strDescId);

			var strTagsId = getTagsId(checkbox_element.index);
			var tags_element = document.getElementById(strTagsId);

			var strAdultId = getAdultContentCheckId(checkbox_element.index);
			var adult_check_element = document.getElementById(strAdultId);

			if (upload_check_element && title_element && desc_element && tags_element && adult_check_element)
			{
				if (checkbox_element.checked == true)
				{
					upload_check_element.checked = false;
					upload_check_element.disabled = true;
					title_element.disabled = true;
					desc_element.disabled = true;
					tags_element.disabled = true;
					adult_check_element.disabled = true;
				}
				else
				{
					// Never enable if we're disconnected.
					if (!bDisconnected)
					{
						upload_check_element.disabled = false;
					}
					title_element.disabled = false;
					desc_element.disabled = false;
					tags_element.disabled = false;
					adult_check_element.disabled = false;
				}
			}
		}

		function GetCheckedHelper(strCheckId)
		{
			var checkbox_element = document.getElementById(strCheckId);
			if (checkbox_element)
			{
				if (checkbox_element.checked)
				{
					return "true";
				}
			}
			return "false";
		}

		function IsDeleteChecked(nIndex)
		{
			var strDeleteCheckId = getDeleteCheckId(nIndex);
			return GetCheckedHelper(strDeleteCheckId);
		}

		function IsUploadChecked(nIndex)
		{
			//Since there is no more upload checkbox, by default all will be uploaded unless marked for deletion
			var strUploadedCheckId = getUploadedCheckId(nIndex);
			var strIsUploadedChecked = GetCheckedHelper(strUploadedCheckId);
			var strDeleteCheckId = getDeleteCheckId(nIndex);
			var strIsDeleteChecked = GetCheckedHelper(strDeleteCheckId);
			if (strIsDeleteChecked == "true" || strIsUploadedChecked == "true")
			{
				strIsUploadChecked = "false";
			}
			else
			{
				strIsUploadChecked = "true";
			}
			return strIsUploadChecked;
		}
		function IsUploadtoYoutube(nIndex)
		{
			var strYoutubeUploadCheckId = getYoutubeUploadCheckId(nIndex);
			return GetCheckedHelper(strYoutubeUploadCheckId);
		}

